; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://Scenes/Combat/Bullet.gd"
}, {
"base": "TileMap",
"class": "DestructibleTilemap",
"language": "GDScript",
"path": "res://Scenes/Utils/DestructibleTileMap.gd"
}, {
"base": "Node2D",
"class": "GameRoom",
"language": "GDScript",
"path": "res://Scenes/Room/GameRoom.gd"
}, {
"base": "Node2D",
"class": "PixelLight",
"language": "GDScript",
"path": "res://Scenes/Utils/Light.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://Scenes/Player/Player.gd"
}, {
"base": "Camera2D",
"class": "ShakeCamera2D",
"language": "GDScript",
"path": "res://Scenes/Player/Cam.gd"
} ]
_global_script_class_icons={
"Bullet": "",
"DestructibleTilemap": "",
"GameRoom": "",
"PixelLight": "",
"Player": "",
"ShakeCamera2D": ""
}

[application]

config/name="BulletHellWeb"
run/main_scene="res://World.tscn"
config/icon="res://icon.png"

[autoload]

RoomsManager="*res://Autoload/RoomsManager.tscn"
ObjectManager="*res://Autoload/ObjectManager.gd"

[display]

window/size/width=180
window/size/height=320
window/size/test_width=360
window/size/test_height=640
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

default=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[layer_names]

2d_physics/layer_1="environment"
2d_physics/layer_2="player"
2d_physics/layer_3="bullets"
2d_physics/layer_4="enemy"

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.0941176, 0.109804, 0.160784, 1 )
environment/default_environment="res://env.tres"
